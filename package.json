{
  "name": "react-shepherd",
  "version": "0.0.1",
  "description": "Declarative Guide/Walktrhough component for React",
  "author": "Morphexe",
  "license": "MIT",
  "repository": "Morphexe/react-shepherd",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "start:docs": "catalog start docs",
    "testCi": "CI=1 react-scripts test --env=jsdom",
    "test": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy:example": "cd example && yarn install && yarn run build",
    "deploy:example": "gh-pages -d example/build",
    "gh-pages": "catalog build docs",
    "predeploy": "catalog build --public-url=/react-shepherd/ docs",
    "deploy": "gh-pages -d docs/build",
    "linter": "eslint --fix src/**/*.{js,jsx}"
  },
  "dependencies": {
    "tether-shepherd": "1.8.1",
    "styled-components": "3.2.5",
    "recompose": "0.26.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.3.0",
    "react-dom": "^15.0.0 || ^16.3.0"
  },
  "devDependencies": {
    "react-github-corner": "2.1.0",

    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "catalog": "3.4.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "gh-pages": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.1.1",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "rollup-plugin-url": "^1.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.3.0"
  },
  "files": ["dist"]
}
